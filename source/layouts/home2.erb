


<% language_tabs = current_page.data.language_tabs || [] %>
<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title><%= current_page.data.title || "Bonsai Documentation" %></title>
    <style>
      <%= Rouge::Themes::Base16::Monokai.render(:scope => '.highlight') %>
    </style>
    <%= stylesheet_link_tag :screen, media: :screen %>
    <%= stylesheet_link_tag :print, media: :print %>
	  <%= stylesheet_link_tag :test, media: :screen %>
    <%= favicon_tag '/favicon.ico' %>
    <% if current_page.data.search %>
      <%= javascript_include_tag  "all" %>
    <% else %>
      <%= javascript_include_tag  "all_nosearch" %>
    <% end %>
	
		<%= partial "partials/google-gtm" %>
	
  </head>

  <body class="<%= page_classes %>" data-languages="<%=h language_tabs.map{ |lang| lang.is_a?(Hash) ? lang.keys.first : lang }.to_json %>">

<%= partial "partials/hide-gtm" %>

  <div id="flex-container">


	<%= partial "partials/primary-nav" %>

	<div id="main-wrapper">


	<div id="top">
	<p class="welcome">Welcome to Bonsai Docs</p>
	<gcse:search></gcse:search>
	
</div>


	<div id="browse">
		<span id="browse-divs">

		<div>
			<p class="p2">Inkling Docs</p>
			<ul>
				<li><% link_to '/inkling-guide.html#inkling-a-new-language.html' do %>Inkling: A New Language<% end %></li>
				<li><% link_to '/inkling-guide.html#curriculum-and-lessons' do %>Curriculum & Lessons<% end %></li>
				<li><% link_to '/ai-engine-guide.html#under-the-hood' do %>Under the Hood<% end %></li>
			</ul>
		</div>


		<div>
			<p class="p2">References</p>
			<ul>
				<li><% link_to '/api-reference.html' do %>API Reference<% end %></li>
				<li><% link_to '/cli-reference.html' do %>CLI Reference<% end %></li>
				<li><% link_to '/inkling-reference.html' do %>Inkling Reference<% end %></li>
			</ul>
		</div>


		<div>
			<p class="p2">Get Help</p>
			<ul>
				<li><% link_to '/examples.html' do %>See Our Examples<% end %></li>
				<li><% link_to 'http://forums.bons.ai/' do %>Ask in the forums<% end %></li>
				<li><% link_to 'mailto:support@bons.ai?subject=I need help with' do %>Send us an email<% end %></li>
			</ul>
		</div>
</span>

	</div>

    </div>

  </div>

  </body>
</html>
